import std::rt;
import std::traits(const);
import std::types(char, u32);

alias str = *const<char>;

def loop<T>(range: &const<vec<T>>) {
    for each <| range.iter()
        rt::print(each.to_string());
}

def main {
    var [argc: u32, argv: *str] = rt::sys::args();
    var arg_vec = rt::vec<str>::new(argv, argc);
    loop<str>(arg_vec);

    var l1 = () => 5;
    let l2 = () { return 5; };
    let l3 = () <| 5;
    let l4 = () [] -> type { };
}

